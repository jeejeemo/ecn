<?php

$array = ["Alain","Eric","Jennifer","Paul"];

// Pour ceux qui ont fini

$array = [
  ["Alain"],
  "Eric",
  "Jennifer",
  "Paul",
  ["monIndex"=>"test"]
];

$itemCount = 4;

$i=0;
while($i<$itemCount){
    echo $array[$i] . "<br>";
    $i++;
}

 ?>

